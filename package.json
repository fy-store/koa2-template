{
	"type": "module",
	"name": "koa2-template",
	"version": "2.0.1-dev",
	"description": "koa2后端模板",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon --exec tsx -r tsconfig-paths/register ./system/dev/main.ts",
		"debug": "cross-env NODE_ENV=development nodemon --exec tsx -r tsconfig-paths/register --inspect ./system/dev/main.ts",
		"start": "tsx ./system/main.ts",
		"key": "tsx ./system/createKey/index.ts",
		"build": "tsc"
	},
	"keywords": [
		"koa2-template",
		"koa-template",
		"koa",
		"node"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/ali-oss": "^6.16.11",
		"@types/bcrypt": "^5.0.2",
		"@types/ip": "^1.1.3",
		"@types/js-yaml": "^4.0.9",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/koa": "^2.15.0",
		"@types/koa-router": "^7.4.8",
		"@types/koa-static": "^4.0.4",
		"@types/node": "^20.12.12",
		"clipboardy": "^4.0.0",
		"cross-env": "^7.0.3",
		"nodemon": "^3.1.1",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.11.0",
		"typescript": "^5.4.5",
		"webpack": "^5.91.0"
