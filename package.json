		"koa": "^2.15.3",
		"koa-router": "^12.0.1",
		"koa-static": "^5.0.0",
		"lodash": "^4.17.21",
		"log4js": "^6.9.1",
		"mysql2": "^3.9.9",
		"sequelize": "^6.37.3",
		"xss": "^1.0.15"
