{
	"type": "module",
	"name": "koa2-template",
	"version": "2.0.9-dev",
	"description": "基于 koa2 构建的 nodejs 后端模版",
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon --exec tsx -r tsconfig-paths/register ./system/dev/main.ts",
		"dev:start": "tsx --inspect ./system/main.ts",
		"debug": "cross-env NODE_ENV=development nodemon --exec tsx -r tsconfig-paths/register --inspect ./system/dev/main.ts",
		"key": "tsx ./system/createKey/index.ts",
		"build": "node ./buildConfig/init.js && rollup --config ./buildConfig/rollup.config.js",
		"preview": "npm run build && node ./dist/src/main.js"
	},
	"keywords": [
		"koa2-template",
		"koa-template",
		"koa",
		"node"
	],
	"author": "feiYu",
	"license": "MIT",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/ali-oss": "^6.16.11",
		"@types/bcrypt": "^5.0.2",
		"@types/ip": "^1.1.3",
		"@types/js-yaml": "^4.0.9",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/koa": "^2.15.0",
		"@types/koa-router": "^7.4.8",
		"@types/koa-static": "^4.0.4",
		"@types/node": "^20.12.12",
		"clipboardy": "^4.0.0",
		"cross-env": "^7.0.3",
		"nodemon": "3.1.1",
		"rollup": "^4.18.0",
		"tsconfig-paths": "^4.2.0",
		"tslib": "^2.6.3",
		"tsx": "^4.11.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@koa/bodyparser": "^5.1.1",
		"@types/lodash-es": "^4.17.12",
		"ali-oss": "^6.20.0",
		"assist-tools": "^2.3.0",
		"bcrypt": "^5.1.1",
		"chalk": "^5.3.0",
		"formidable": "^3.5.1",
		"ip": "^2.0.1",
		"js-yaml": "^4.1.0",
		"jsonwebtoken": "^9.0.2",
		"koa": "^2.15.3",
		"koa-router": "^12.0.1",
		"koa-static": "^5.0.0",
		"koa2-template": "link:",
		"lodash": "^4.17.21",
		"lodash-es": "^4.17.21",
		"log4js": "^6.9.1",
		"mysql2": "^3.9.9",
		"nanoid": "^5.0.7",
		"sequelize": "^6.37.3",
		"xss": "^1.0.15"
	},
	"volta": {
		"node": "20.14.0"
	}
}
